{
  "coc.preferences.formatOnSave": true,
  "suggest.noselect": true,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "typescript.suggest.includeCompletionsWithSnippetText": false,
  "typescript.suggest.includeCompletionsForImportStatements": false,
  "cssmodules.camelCase": true,
  "cSpell.userWords": [
    "Laursen",
    "Mikkel",
    "Mikkel's",
    "activedescendant",
    "autocomplete",
    "browserslist",
    "cloneable",
    "cnbuilder",
    "columnheader",
    "describedby",
    "dropzone",
    "filesize",
    "flexbox",
    "focusable",
    "greyscale",
    "haspopup",
    "hoverable",
    "interactable",
    "keyline",
    "labelledby",
    "listbox",
    "listitem",
    "loopable",
    "macbook",
    "menubutton",
    "menuitemradio",
    "mlaursen",
    "oidc",
    "pnpm",
    "posinset",
    "preconfigured",
    "prismjs",
    "reduxjs",
    "renderable",
    "roledescription",
    "sassdoc",
    "screenreader",
    "setsize",
    "signin",
    "signout",
    "stylelint",
    "subheaders",
    "svgs",
    "tabindex",
    "tabpanel",
    "testid",
    "textbox",
    "toggleable",
    "treeitem",
    "treeitems",
    "typecheck",
    "typedoc",
    "typeparam",
    "unstyled",
    "upsert",
    "upsertable",
    "valuenow"
  ],
  "tsserver.useLocalTsdk": true,
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": ["typescript", "typescriptreact", "graphql"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"]
    }
  },
  "yaml.schemas": {
    "https://gitlab.com/gitlab-org/gitlab/-/raw/master/app/assets/javascripts/editor/schema/ci.json": [
      "/gitlab/*.yml",
      "/gitlab/*.yaml"
    ]
  }
}
